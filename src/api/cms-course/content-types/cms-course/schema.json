{
  "kind": "collectionType",
  "collectionName": "cms_courses",
  "info": {
    "singularName": "cms-course",
    "pluralName": "cms-courses",
    "displayName": "CMS-course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course_name": {
      "type": "uid"
    },
    "page_data": {
      "type": "dynamiczone",
      "components": [
        "common.gallery",
        "common.google-ads",
        "common.navbar",
        "common.new-overview",
        "common.faq"
      ]
    },
    "average_fees": {
      "type": "string"
    },
    "is_top_course": {
      "type": "boolean",
      "default": false
    },
    "duration": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Bachelors",
        "Masters",
        "Doctorate",
        "Diploma",
        "Certification"
      ]
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "canonical": {
      "type": "string"
    },
    "keywords": {
      "type": "string"
    },
    "og_title": {
      "type": "string"
    },
    "og_url": {
      "type": "string"
    },
    "og_description": {
      "type": "string"
    },
    "og_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "og_image_alt": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "og_image_alt_text": {
      "type": "string"
    }
  }
}
